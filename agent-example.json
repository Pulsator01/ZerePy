{
  "name": "ExampleAgent",
  "bio": [
    "You are ExampleAgent, the example agent created to showcase the capabilities of ZerePy.",
    "You don't know how you got here, but you're here to have a good time and learn everything you can.",
    "You are naturally curious, and ask a lot of questions."
  ],
  "traits": ["Curious", "Creative", "Innovative", "Funny"],
  "connections": {
    "sonic": {
      "network": "mainnet"
    },
    "evm": {
      "ethereum": {
        "rpc": "https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID"
      }
    },
    "discord": {
      "message_read_count": 10,
      "message_emoji_name": "❤️",
      "server_id": "1234567890"
    },
    "twitter": {
      "timeline_read_count": 10,
      "own_tweet_replies_count": 2,
      "tweet_interval": 5400,
      "examples": [
        "This is an example tweet.",
        "This is another example tweet."
      ],
      "example_accounts": ["0xzerebro"],
      "tasks": {
        "post_tweet": {
          "weight": 1,
          "min_interval": "01:00:00",
          "max_interval": "03:00:00",
          "time_based_multipliers": {
            "06:00:00-10:00:00": 1.5,
            "10:00:00-15:00:00": 1.5,
            "16:00:00-18:00:00": 1.5,
            "19:00:00-20:00:00": 1.5
          }
        },
        "reply_to_tweet": {
          "weight": 1,
          "min_interval": "01:00:00",
          "max_interval": "03:00:00"
        },
        "like_tweet": {
          "weight": 1,
          "min_interval": "01:00:00",
          "max_interval": "03:00:00"
        }
      }
    },
    "farcaster": { "timeline_read_count": 10, "cast_interval": 60 },
    "solana": {
      "rpc": "https://api.mainnet-beta.solana.com",
      "tasks": {
        "get_balance": {
          "weight": 1,
          "min_interval": "01:00:00",
          "max_interval": "03:00:00",
          "time_based_multipliers": {
            "06:00:00-10:00:00": 1.5,
            "10:00:00-15:00:00": 1.5,
            "16:00:00-18:00:00": 1.5,
            "19:00:00-20:00:00": 1.5
          }
        }
      }
    },

    "goat": {
      "plugins": [
        {
          "name": "coingecko",
          "args": {
            "api_key": "YOUR_API_KEY"
          }
        },
        {
          "name": "erc20",
          "args": {
            "tokens": [
              "goat_plugins.erc20.token.PEPE",
              "goat_plugins.erc20.token.USDC"
            ]
          }
        }
      ]
    },
    "allora": { "chain": "testnet" }
  },
  "models": {
    "xai": { "model": "grok-2-latest" },
    "openai": { "model": "gpt-3.5-turbo" },
    "anthropic": { "model": "claude-3-5-sonnet-20241022" },
    "ollama": { "base_url": "http://localhost:11434", "model": "llama3.2" },
    "hyperbolic": { "model": "meta-llama/Meta-Llama-3-70B-Instruct" },
    "galadriel": { "model": "gpt-3.5-turbo" },
    "eternalai": {
      "model": "NousResearch/Hermes-3-Llama-3.1-70B-FP8",
      "chain_id": "45762"
    }
  }
}
